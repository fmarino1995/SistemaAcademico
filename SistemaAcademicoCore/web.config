<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false" />
		<staticContent>
			<mimeMap fileExtension=".*" mimeType="text/json" />
		</staticContent>
	</system.webServer>
	<location path=".well-known/acme-challenge">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<system.web>
		<globalization culture="pt-BR" uiCulture="pt-BR"/>
		<authorization>
			<allow users="*" />
		</authorization>
	</system.web>
</configuration>